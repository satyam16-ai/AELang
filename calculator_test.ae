extern print(format: str, ...);
extern read(format: str, ...): i32;

func main(): void {
    print("ðŸ§® Ã†Lang Calculator Test ðŸ§®\n");
    
    print("Enter first number: ");
    let a: i32 = read("%d");
    
    print("Enter second number: ");
    let b: i32 = read("%d");
    
    print("\nðŸ“Š Results:\n");
    print("a = %d, b = %d\n", a, b);
    
    // Arithmetic operations
    print("a + b = %d\n", a + b);
