// Debug test for function parameters
extern print(message: str);
extern print_int(val: i32);

func test_add(a: i32, b: i32): i32 {
    return a + b;
}

func main(): void {
    print("Testing function calls:");
    let result: i32 = test_add(10, 5);
    print("10 + 5 should be 15:");
    print_int(result);
    
    print("Direct call test_add(3, 7):");
    print_int(test_add(3, 7));
}
