extern print(format: str, ...);

func main(): void {
    print("=== Simple Arithmetic Test ===\n");
    
    let a: i32 = 10;
    let b: i32 = 5;
    let c: i32 = 3;
    let d: i32 = 2;
    
    print("Variables: a=%d, b=%d, c=%d, d=%d\n", a, b, c, d);
    
    // Test simple expressions first
    let test1: i32 = b * c;
    print("b * c = %d * %d = %d\n", b, c, test1);
    
    let test2: i32 = d / d;
    print("d / d = %d / %d = %d\n", d, d, test2);
    
    let test3: i32 = a + test1;
    print("a + (b * c) = %d + %d = %d\n", a, test1, test3);
    
    let test4: i32 = test3 - test2;
    print("(a + b * c) - (d / d) = %d - %d = %d\n", test3, test2, test4);
    
    // Now test the full expression
    let precedence_result: i32 = a + b * c - d / d;
    print("Full expression: %d + %d * %d - %d / %d = %d\n", a, b, c, d, d, precedence_result);
    print("Expected: 10 + 15 - 1 = 24\n");
}
