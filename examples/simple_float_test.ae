// Simple float test to isolate the NaN issue
extern print(msg: str);
extern print_clean(msg: str);
extern print_num(val: num);
extern print_int(val: i32);

func main(): void {
    print("Simple Float Test:");
    
    // Test basic float literal
    let a: f32 = 3.14159;
    print_clean("Float a = "); print_num(a);
    print("");
    
    // Test integer factorial
    print("Factorial Test:");
    let n: i32 = 5;
    let factorial_result: i32 = 1;
    
    print_clean("n = "); print_int(n);
    print_clean("Starting factorial_result = "); print_int(factorial_result);
    
    if n >= 1 { 
        factorial_result = factorial_result * 1; 
        print_clean("After *1: "); print_int(factorial_result);
    }
    if n >= 2 { 
        factorial_result = factorial_result * 2; 
        print_clean("After *2: "); print_int(factorial_result);
    }
    if n >= 3 { 
        factorial_result = factorial_result * 3; 
        print_clean("After *3: "); print_int(factorial_result);
    }
    if n >= 4 { 
        factorial_result = factorial_result * 4; 
        print_clean("After *4: "); print_int(factorial_result);
    }
    if n >= 5 { 
        factorial_result = factorial_result * 5; 
        print_clean("After *5: "); print_int(factorial_result);
    }
    
    print_clean("Final result: "); print_int(factorial_result);
}
