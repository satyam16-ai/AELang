extern print_int(val: i32);

func main(): void {
    print("=== Complex Expression Test ===");
    
    // Test nested expressions and parentheses
    let a: i32 = 10;
    let b: i32 = 5;
    let c: i32 = 3;
    
    // Simple arithmetic
    let simple: i32 = a + b;
    print("Simple: a + b =");
    print_int(simple);
    
    // Complex expression with parentheses
    let complex: i32 = (a + b) * (c - 1);
    print("Complex: (a + b) * (c - 1) =");
    print_int(complex);
    
    // Deeply nested
    let nested: i32 = ((a + b) * c) + ((a - b) / c);
    print("Nested: ((a + b) * c) + ((a - b) / c) =");
    print_int(nested);
    
    print("Complex expression test completed!");
}
