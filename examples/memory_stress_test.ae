extern print_int(val: i32);
extern print_float(val: f32);

func main(): void {
    print("=== Ã†Lang Memory Stress Test ===");
    print("Testing variable allocation limits");
    print("");
    
    // Test 1: Multiple integer variables
    let a1: i32 = 1;
    let a2: i32 = 2;
    let a3: i32 = 3;
    let a4: i32 = 4;
    let a5: i32 = 5;
    let a6: i32 = 6;
    let a7: i32 = 7;
    let a8: i32 = 8;
    let a9: i32 = 9;
    let a10: i32 = 10;
    
    // Test 2: Multiple float variables
    let b1: f32 = 1.1;
    let b2: f32 = 2.2;
    let b3: f32 = 3.3;
    let b4: f32 = 4.4;
    let b5: f32 = 5.5;
    
    // Test 3: Arithmetic operations with many variables
    let sum1: i32 = a1 + a2 + a3;
    let sum2: i32 = a4 + a5 + a6;
    let sum3: i32 = a7 + a8 + a9 + a10;
    
    let fsum1: f32 = b1 + b2;
    let fsum2: f32 = b3 + b4 + b5;
    
    // Test 4: Display results
    print("Integer sums:");
    print_int(sum1);
    print_int(sum2);
    print_int(sum3);
    
    print("Float sums:");
    print_float(fsum1);
    print_float(fsum2);
    
    print("Memory allocation test completed!");
}
