extern print_int(val: i32);
func main(): void {
    let a: i32 = 20;
    let b: i32 = 4;
    
    // Test addition
    let add_result: i32 = a + b;
    print_int(add_result);
    
    // Test subtraction
    let sub_result: i32 = a - b;
    print_int(sub_result);
    
    // Test multiplication
    let mul_result: i32 = a * b;
    print_int(mul_result);
    
    // Test division
    let div_result: i32 = a / b;
    print_int(div_result);
}
