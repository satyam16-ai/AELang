// Simple 64-bit Architecture Test

func main(): void {
    let test_i64: i64 = 9223372036854775807;    // Max i64 value
    let test_u64: u64 = 18446744073709551615;   // Max u64 value
    let test_f64: f64 = 123.456789012345;       // Double precision value
    let test_char: char = 'Z';                  // Character test
    let test_str: str = "64-bit test";          // String test
    let test_bool: bool = false;                // Boolean test

    print_str("=== Simple 64-bit Architecture Test ===");
    print_char('\n');

    print_str("Max i64: ");
    print_i64(test_i64);
    print_char('\n');

    print_str("Max u64: ");
    print_u64(test_u64);
    print_char('\n');

    print_str("f64 value: ");
    print_f64(test_f64);
    print_char('\n');

    print_str("Character: ");
    print_char(test_char);
    print_char('\n');

    print_str("String: ");
    print_str(test_str);
    print_char('\n');

    print_str("Boolean: ");
    print_bool(test_bool);
    print_char('\n');

    print_str("=== 64-bit Test Complete ===");
    print_char('\n');
}
