extern print_int(val: i32);
extern read_int(): i32;

func main(): void {
    print("Enter first number: ");
    let a: i32 = read_int();
    
    print("Enter second number: ");
    let b: i32 = read_int();
    
    let sum: i32 = a + b;
    let diff: i32 = a - b;
    let product: i32 = a * b;
    let quotient: i32 = a / b;
    
    print("Results:");
    print("Sum: ");
    print_int(sum);
    
    print("Difference: ");
    print_int(diff);
    
    print("Product: ");
    print_int(product);
    
    print("Quotient: ");
    print_int(quotient);
}
