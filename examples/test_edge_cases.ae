extern print(format: str, ...);
extern read();

func main(): void {
    print("=== EDGE CASE TESTING ===\n");
    
    // Test boundary values for each type
    print("Testing i8 minimum (-128): ");
    let test_i8_min: i8 = read();
    print("Result: %d\n", test_i8_min);
    
    print("Testing i8 maximum (127): ");
    let test_i8_max: i8 = read();
    print("Result: %d\n", test_i8_max);
    
    print("Testing u8 maximum (255): ");
    let test_u8_max: u8 = read();
    print("Result: %d\n", test_u8_max);
    
    print("Testing i16 minimum (-32768): ");
    let test_i16_min: i16 = read();
    print("Result: %d\n", test_i16_min);
    
    print("Testing i16 maximum (32767): ");
    let test_i16_max: i16 = read();
    print("Result: %d\n", test_i16_max);
    
    print("Testing u16 maximum (65535): ");
    let test_u16_max: u16 = read();
    print("Result: %d\n", test_u16_max);
    
    print("Testing i32 minimum (-2147483648): ");
    let test_i32_min: i32 = read();
    print("Result: %d\n", test_i32_min);
    
    print("Testing i32 maximum (2147483647): ");
    let test_i32_max: i32 = read();
    print("Result: %d\n", test_i32_max);
    
    print("Testing u32 maximum (4294967295): ");
    let test_u32_max: u32 = read();
    print("Result: %u\n", test_u32_max);
    
    print("Testing small float values:\n");
    print("Enter 0.001: ");
    let small_f32: f32 = read();
    print("f32 result: %f\n", small_f32);
    
    print("Enter 0.001 for f8: ");
    let small_f8: f8 = read();
    print("f8 result: %f\n", small_f8);
    
    print("Enter 0.001 for f16: ");
    let small_f16: f16 = read();
    print("f16 result: %f\n", small_f16);
    
    print("Testing negative floats:\n");
    print("Enter -3.14159: ");
    let neg_f32: f32 = read();
    print("f32 result: %f\n", neg_f32);
    
    print("Enter -1.5 for f8: ");
    let neg_f8: f8 = read();
    print("f8 result: %f\n", neg_f8);
    
    print("Enter -2.7 for f16: ");
    let neg_f16: f16 = read();
    print("f16 result: %f\n", neg_f16);
    
    print("Testing zero values:\n");
    print("Enter 0 for i32: ");
    let zero_i32: i32 = read();
    print("i32 result: %d\n", zero_i32);
    
    print("Enter 0.0 for f32: ");
    let zero_f32: f32 = read();
    print("f32 result: %f\n", zero_f32);
    
    print("Enter 0.0 for f8: ");
    let zero_f8: f8 = read();
    print("f8 result: %f\n", zero_f8);
    
    print("Enter 0.0 for f16: ");
    let zero_f16: f16 = read();
    print("f16 result: %f\n", zero_f16);
    
    print("Testing boolean edge cases:\n");
    print("Enter false: ");
    let test_false: bool = read();
    print("bool result: %t\n", test_false);
    
    print("Enter true: ");
    let test_true: bool = read();
    print("bool result: %t\n", test_true);
    
    print("Testing character edge cases:\n");
    print("Enter space character: ");
    let test_space: char = read();
    print("char result: '%c'\n", test_space);
    
    print("Enter digit '9': ");
    let test_digit: char = read();
    print("char result: '%c'\n", test_digit);
    
    print("\n=== EDGE CASE TESTING COMPLETE ===\n");
}
