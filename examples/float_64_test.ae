extern print(format: str, ...);

func main(): void {
    print("=== 64-bit Float Test ===\\n");
    
    // Test direct float values
    print("Direct float: %f\\n", 123.45);
    print("Currency: %$\\n", 123.45);
    print("Percentage: %P\\n", 0.85);
    
    // Test variables
    let price: f32 = 456.78;
    print("Variable float: %f\\n", price);
    print("Variable currency: %$\\n", price);
}