// Test: Truly Universal Read System
// This test verifies that read() calls are preserved without transformation

i32 age;
f32 height; 
bool is_student;
str name;

age = read();
height = read(); 
is_student = read();
name = read();

print("Age: ", age, "\n");
print("Height: ", height, "\n");
print("Student: ", is_student, "\n");  
print("Name: ", name, "\n");
