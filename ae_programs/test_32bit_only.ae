extern print(format: str, ...);
extern read(): num;

func test_32bit_types(): void {
    print("=== Testing 32-bit Compatible Types Only ===\n");
    
    // Only 32-bit and smaller types
    let test_i8: i8 = 127;
    let test_i16: i16 = 32767;
    let test_i32: i32 = 2147483647;
    
    let test_u8: u8 = 255;
    let test_u16: u16 = 65535;
    let test_u32: u32 = 4294967295;
    
    let test_f32: f32 = 3.14159;
    let test_f8: f8 = 2.5;
    let test_f16: f16 = 15.625;
    
    print("i8: %d\n", test_i8);
    print("i16: %d\n", test_i16);
    print("i32: %d\n", test_i32);
    print("u8: %u\n", test_u8);
    print("u16: %u\n", test_u16);
    print("u32: %u\n", test_u32);
    print("f32: %f\n", test_f32);
    print("f8: %f\n", test_f8);
    print("f16: %f\n", test_f16);
    
    print("Enter a number: ");
    let user_input: num = read();
    print("You entered: %f\n", user_input);
}

func main(): void {
    test_32bit_types();
}
