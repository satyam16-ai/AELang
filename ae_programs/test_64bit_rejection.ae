extern print(format: str, ...);

func test_64bit_rejection(): void {
    print("Testing 64-bit type rejection in 32-bit mode\n");
    
    // This should cause compilation error in 32-bit mode
    let test_i64: i64 = 9223372036854775807;
    
    print("i64: %ld\n", test_i64);
}

func main(): void {
    test_64bit_rejection();
}
