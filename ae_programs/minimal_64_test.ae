extern print(format: str, ...);

func main(): void {
    print("=== Minimal 64-bit Test ===\\n");
    
    // Test basic print functionality step by step
    print("Step 1: About to test integer\\n");
    print("Integer: %d\\n", 42);
    
    print("Step 2: About to test binary\\n");  
    print("Binary: %b\\n", 42);
    
    print("Step 3: About to test boolean\\n");
    print("Boolean: %t\\n", true);
    
    print("Step 4: About to test variable\\n");
    let number: i32 = 123;
    print("Variable: %d\\n", number);
    
    print("All basic features work in 64-bit!\\n");
}
