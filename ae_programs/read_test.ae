extern print(format: str, ...);
extern read();
func main(): void {
    // Take two inputs and perform all basic arithmetic operations
    print("Welcome to the AE Calculator!\n");
    print("Please enter two integers:\n");
    print("Enter first integer: ");
    let a: i32 = read();
    print("Enter second integer: ");
    let b: i32 = read();
    print("You entered: %d and %d\n", a, b);
    // Simple arithmetic operations
    print("Addition: %d\n", a + b);
    print("Subtraction: %d\n", a - b);
    print("Multiplication: %d\n", a * b);
    print("Division: %d\n", a / b);
    print("Calculator operations complete!\n");
}